<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper>
    <insert id="insertLoginUser" useGeneratedKeys="true" parameterType="LoginUser"  >
        <selectKey keyProperty="id" order="BEFORE" resultType="java.lang.String">
            select UUID()
        </selectKey>
        insert into login_user(id,username,password) values (#{id},#{username},#{password})
    </insert>
</mapper>